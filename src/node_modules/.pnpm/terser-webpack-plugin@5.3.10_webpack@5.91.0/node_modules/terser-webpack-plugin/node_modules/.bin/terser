#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules/terser/bin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules/terser/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/node_modules:/home/basic-app/todo-app/node_modules:/home/basic-app/node_modules:/home/node_modules:/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser-webpack-plugin@5.3.10_webpack@5.91.0/node_modules/terser-webpack-plugin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser-webpack-plugin@5.3.10_webpack@5.91.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules/terser/bin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules/terser/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser@5.30.3/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/node_modules:/home/basic-app/todo-app/node_modules:/home/basic-app/node_modules:/home/node_modules:/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser-webpack-plugin@5.3.10_webpack@5.91.0/node_modules/terser-webpack-plugin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/terser-webpack-plugin@5.3.10_webpack@5.91.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.30.3/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@5.30.3/node_modules/terser/bin/terser" "$@"
fi
