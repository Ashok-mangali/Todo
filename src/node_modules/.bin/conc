#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/bin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/node_modules:/home/basic-app/todo-app/node_modules:/home/basic-app/node_modules:/home/node_modules:/node_modules:/home/basic-app/todo-app/node_modules/concurrently/dist/bin/node_modules:/home/basic-app/todo-app/node_modules/concurrently/dist/node_modules:/home/basic-app/todo-app/node_modules/concurrently/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/bin/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/concurrently@8.2.2/node_modules:/home/basic-app/todo-app/node_modules/.pnpm/node_modules:/home/basic-app/todo-app/node_modules:/home/basic-app/node_modules:/home/node_modules:/node_modules:/home/basic-app/todo-app/node_modules/concurrently/dist/bin/node_modules:/home/basic-app/todo-app/node_modules/concurrently/dist/node_modules:/home/basic-app/todo-app/node_modules/concurrently/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../concurrently/dist/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../concurrently/dist/bin/concurrently.js" "$@"
fi
